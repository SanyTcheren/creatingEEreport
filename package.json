{
  "name": "creatingeereport",
  "version": "0.0.0",
  "description": "creating a report on the consumption of electrical energy when drilling oil wells",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/src/main.js",
    "build": "tsc | cp -r ./public ./dist",
    "test": "jest",
    "dev": "nodemon",
    "lint": "selint ./src**",
    "lint:fix": "eslint ./src/** --fix",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9229 -r ts-node/register src/main.ts",
    "doctor": "clinic doctor --on-port 'autocannon -m GET localhost:8000/hello' -- node dist/main.js",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage",
    "generate": "npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SanyTcheren/creatingEEreport.git"
  },
  "keywords": [
    "report",
    "energy",
    "creating",
    "electrical",
    "oil"
  ],
  "author": "Sany Tcheren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SanyTcheren/creatingEEreport/issues"
  },
  "homepage": "https://github.com/SanyTcheren/creatingEEreport#readme",
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@types/ejs": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^15.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-fileupload": "^1.3.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mustard-ui": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
